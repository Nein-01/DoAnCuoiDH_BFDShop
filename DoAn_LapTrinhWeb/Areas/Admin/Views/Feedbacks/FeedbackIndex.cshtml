@using PagedList.Mvc
@model PagedList.IPagedList<DoAn_LapTrinhWeb.DTOs.FeedbackDTOs>
@{
    ViewBag.Title = "Đánh giá sản phẩm";
    Layout = "~/Areas/Admin/Views/Shared/LayoutAdmin.cshtml";
    var defaultImg = "/Images/ImagesCollection/no-image-available.png";
    var reply_fb = (List<DoAn_LapTrinhWeb.Model.Feedback>)ViewBag.reply_fb;
    var fb_img = (List<DoAn_LapTrinhWeb.Model.Feedback_Image>)ViewBag.fb_img;
}

@section scripts{
    <script src="~/Scripts/admin_js/list_feedback.js"></script>
    <script>
        // Element to indecate
        var button = document.querySelector("#CancleBtn");
        // Handle button click event
        button.addEventListener("click", function () {
            // Activate indicator
            button.setAttribute("data-kt-indicator", "on");

            // Disable indicator after 3 seconds
            setTimeout(function () {
                button.removeAttribute("data-kt-indicator");
            }, 30000);
        });
    </script>

}

